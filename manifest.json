{
  "manifest_version": 3,
  "name": "Create markdown link",
  "short_name":"Create MD link",
  "version": "1.0.0",
  "icons": {
      "16": "logo/copy-icon.png",
      "48": "logo/copy-icon.png",
      "128": "logo/128.png"
  },
  "description": "Create markdown link from selected text",
  "content_scripts": [{
